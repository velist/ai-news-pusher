{
  "version": 2,
  "name": "ai-news-pusher",
  "public": true,
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "routes": [
    {
      "src": "/",
      "dest": "/docs/index.html"
    },
    {
      "src": "/news/(.*)",
      "dest": "/docs/news/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}